datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int       @id @default(autoincrement())
  userId    Int       @unique
  ipRecords IPRecord[]
}

model IPRecord {
  id        Int      @id @default(autoincrement())
  userId    Int
  ip        String
  createdAt DateTime @default(now())
  user      User     @relation(fields: [userId], references: [id])
}
